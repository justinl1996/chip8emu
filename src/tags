INCLUDE	Makefile	/^INCLUDE= -I.$/;"	m
LIBS	Makefile	/^LIBS= -lpthread -lX11$/;"	m
MEM_SIZE	chip8.h	2;"	d
PROG_START	chip8.h	3;"	d
SOURCES	Makefile	/^SOURCES = $(PROJ).c $(SRCS).c$/;"	m
drawFlag	chip8.h	/^    bool drawFlag;$/;"	m	class:Chip8
